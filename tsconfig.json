{
  "compilerOptions": {
    "lib": ["ES2020","DOM"],
    "module": "node16",
    "target": "es2020",
    "baseUrl": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  }
}
